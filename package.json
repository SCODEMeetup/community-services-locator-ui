{
  "name": "community-services-locator-ui",
  "version": "0.0.1",
  "description": "This application is a map to locate all the different services in ohio",
  "author": "Mihir",
  "main": "build",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "node server.js",
    "heroku-postbuild": "webpack -p --env.prod",
    "cleanup": "rm -rf build/* && rm -rf dist/* && rm -rf coverage/*",
    "build": "cross-env NODE_ENV=production webpack --env.prod",
    "test": "cross-env BABEL_ENV=test mocha --require babel-core/register --require test/helpers.js --timeout 10000 src/**/*.spec.js",
    "test-file": "cross-env BABEL_ENV=test mocha --require babel-core/register --require test/helpers.js --timeout 10000",
    "tests-coverage": "cross-env BABEL_ENV=test mocha --require babel-core/register --require test/helpers.js --timeout 10000 coverage/**/*.spec.js",
    "pre-coverage": "cross-env istanbul instrument src --output coverage --no-compact --complete-copy --es-modules",
    "get-coverage": "npm run cleanup && npm run pre-coverage && npm run tests-coverage -- -R test/istanbul-reporter.js",
    "precommit": "lint-staged",
    "bundle": "webpack --config webpack.config.js -p --env.prod",
    "prestart": "npm run bundle"
  },
  "dependencies": {
    "attr-accept": "^1.1.3",
    "express": "^4.16.4",
    "flexbox-react": "^4.4.0",
    "hellojs": "^1.17.1",
    "humanize-plus": "^1.8.2",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dom": "^16.7.0",
    "react-google-maps": "^9.4.5",
    "react-redux": "^6.0.0",
    "react-router5": "^5.4.2",
    "react-toolbox": "^2.0.0-beta.12",
    "react-vis": "^1.11.5",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-router5": "^5.2.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "router5": "^5.8.2",
    "router5-plugin-browser": "^7.0.1",
    "showdown": "^1.9.0",
    "throttle-debounce": "^2.0.1",
    "universal-ga": "^1.2.0",
    "webpack": "^4.28.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.2",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^v4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.1",
    "istanbul": "^1.1.0-alpha.1",
    "istanbul-api": "^2.0.6",
    "istanbul-lib-coverage": "^2.0.1",
    "js-yaml": "^3.12.0",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.15.3",
    "raw-loader": "^1.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "redux-mock-store": "^1.5.3",
    "run-func": "^1.0.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1 ",
    "stylelint": "^9.9.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.4.3",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
